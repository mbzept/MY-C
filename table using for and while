#include <stdio.h>
int main() 
{
    printf("table using for loop\n");
    int n,i=1;
    printf("n=");
    scanf("%d",&n);
    for (int i=1;i<=10;i++){
        printf("%d X %d = %d \n",n,i, n*i);
    }
    printf("\n\n");
    printf("table using while loop\n");
    printf("n=");
    scanf("%d",&n);
    while(i<=10){
        printf("%d X %d = %d\n",n,i,n*i);
        i++;
    }
}
