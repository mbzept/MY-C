#include <stdio.h>
int main()
{
   int a[3][3]={{1,2,3},{4,5,6},{7,8,9}};
   int z[3][3],i,j;
   printf("transpose of matrix\n");
   for (i=0;i<3;i++){
    for(j=0;j<3;j++){
        z[j][i]=a[i][j];
    }
   }
   for (i=0;i<3;i++){
    for(j=0;j<3;j++){
        printf("%d ",z[i][j]);
    }
    printf("\n");
   }
}
